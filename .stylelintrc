{
  "rules": {
    "block-no-empty": null,
    "selector-class-pattern": "^([a-z]+[A-Z]+\\w+)+$"
  },
  "defaultSeverity": "warning",
  "plugins": [
    "stylelint-scss"
  ],
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-standard-scss",
    "stylelint-config-tailwindcss",
    "stylelint-config-tailwindcss/scss"
  ]
}
